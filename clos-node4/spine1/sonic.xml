<domain type='kvm' xmlns:qemu='http://libvirt.org/schemas/domain/qemu/1.0'>
  <!-- ########## VM Config ########## -->
  <name>spine1</name>
  <memory unit='KiB'>4096000</memory>
  <currentMemory unit='KiB'>4096000</currentMemory>
  <vcpu placement='static'>2</vcpu>
  <resource>
    <partition>/machine</partition>
  </resource>
  <os>
    <type arch='x86_64' machine='pc-i440fx-1.5'>hvm</type>
    <boot dev='hd'/>
  </os>
  <features>
    <acpi/>
    <apic/>
  </features>
  <clock offset='utc'/>
  <on_poweroff>destroy</on_poweroff>
  <on_reboot>restart</on_reboot>
  <on_crash>restart</on_crash>
  <devices>
    <emulator>/usr/bin/qemu-system-x86_64</emulator>
    <!-- ########## Disk ########## -->
    <disk type='file' device='disk'>
      <driver name='qemu' type='qcow2' cache='writeback'/>
      <source file='/home/vagrant/share/sonic-setup/sonic-images/sonic-vs3.img'/>
      <target bus='virtio' dev='vda'/>
    </disk>
    <!-- ########## Telnet ########## -->
    <serial type='tcp'>
      <source host='127.0.0.1' mode='bind' service='7011'/>
      <target port='0'/>
      <protocol type='telnet'/>
    </serial>
    <!-- eth0 -->
    <interface type='bridge'>
      <source bridge='br_mgmt'/>
      <model type="virtio"/>
    </interface>
    <!-- Ethernet0 (eth1) -->
    <interface type='bridge'>
      <source bridge="br_s1l1"/>
      <model type="virtio"/>
    </interface>
    <!-- Ethernet4 (eth2) -->
    <interface type='bridge'>
      <source bridge="br_s1l2"/>
      <model type="virtio"/>
    </interface>
    <!-- Ethernet8 (eth3) -->
    <!-- <interface type='bridge'>
      <source bridge="br_s1l1"/>
      <model type="virtio"/>
    </interface> -->
    <!-- Ethernet12 (eth4) -->
    <!-- <interface type='bridge'>
      <source bridge="br_s1l1"/>
      <model type="virtio"/>
    </interface> -->
    
    <controller type='usb' index='0'/>
    <memballoon model='virtio'>
      <alias name='balloon0'/>
      <address type='pci' domain='0x0000' bus='0x00' slot='0x04' function='0x0'/>
    </memballoon>
  </devices>
  <seclabel type='dynamic' model='apparmor' relabel='yes'/>
</domain>

